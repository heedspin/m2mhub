var id_input = $("input[name='quote_item[id]'][value=<%= @quote_item.id %>]");
<% if @quote_item.margin %>
	var row = id_input.closest("tr");
	row.find("td.margin").html("<%= percent0 @quote_item.margin %>%");
	<% if @quote_item.price_set_by_margin %>
		row.find("input#quote_item_price").val("<%= @quote_item.price %>");
	<% end %>
<% end %>
